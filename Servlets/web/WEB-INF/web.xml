<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>fstServlet</servlet-name>
        <servlet-class>fstServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>fstServlet</servlet-name>
        <url-pattern></url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Status</servlet-name>
        <jsp-file>/Status.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Status</servlet-name>
        <url-pattern>/status</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Name</servlet-name>
        <jsp-file>/Name.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Name</servlet-name>
        <url-pattern>/get</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CheckStatus</servlet-name>
        <jsp-file>/Check.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckStatus</servlet-name>
        <url-pattern>/check</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Page</servlet-name>
        <servlet-class>Page</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Page</servlet-name>
        <url-pattern>/page</url-pattern>
    </servlet-mapping>

</web-app>